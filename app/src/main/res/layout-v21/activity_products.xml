<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_products"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/asd"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="0dp"
    android:paddingRight="15dp"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.asus.myapplication.products"
    tools:ignore="RtlHardcoded">

    <GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:alignmentMode="alignBounds"
        android:padding="15dp"
        android:rowCount="8"
        tools:ignore="RtlHardcoded,UselessParent">

        <Button
            android:id="@+id/button11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_columnWeight="0"
            android:layout_gravity="fill_horizontal"
            android:layout_row="0"
            android:layout_rowWeight="1"
            android:autoLink="web"
            android:background="@android:drawable/alert_light_frame"
            android:clickable="true"
            android:linksClickable="true"
            android:onClick="Onclick"
            android:text="@string/panasonic" />

        <Button
            android:id="@+id/button10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_columnWeight="0"
            android:layout_gravity="fill_horizontal"
            android:layout_rowWeight="1"
            android:background="@android:drawable/alert_light_frame"
            android:onClick="baumer"
            android:text="@string/baumer" />

        <Button
            android:id="@+id/button7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_columnWeight="0"
            android:layout_rowWeight="1"
            android:background="@android:drawable/alert_light_frame"
            android:onClick="moog"
            android:text="@string/moog" />

        <Button
            android:id="@+id/button9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_gravity="fill_horizontal"
            android:layout_row="2"
            android:background="@android:drawable/alert_light_frame"
            android:onClick="kubler"
            android:text="@string/kubler" />

        <Button
            android:id="@+id/button12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@android:drawable/alert_light_frame"
            android:onClick="proface"
            android:text="@string/proface" />

        <Button
            android:id="@+id/button14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:layout_row="2"
            android:background="@android:drawable/alert_light_frame"
            android:onClick="cognex"
            android:text="@string/cognex" />

        <Button
            android:id="@+id/button16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_gravity="fill_horizontal"
            android:layout_row="3"
            android:background="@android:drawable/alert_light_frame"
            android:onClick="bosch"
            android:text="@string/bosch" />

        <Button
            android:id="@+id/button17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@android:drawable/alert_light_frame"
            android:onClick="vst"
            android:text="@string/vst" />

        <Button
            android:id="@+id/button18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@android:drawable/alert_light_frame"
            android:onClick="schmidt"
            android:text="@string/schimdt" />

        <Button
            android:id="@+id/button19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_row="4"
            android:background="@android:drawable/alert_light_frame"
            android:onClick="novotechnik"
            android:text="@string/novotechnik" />

        <Button
            android:id="@+id/button20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@android:drawable/alert_light_frame"
            android:onClick="katoro"
            android:text="@string/katoro" />

        <Button
            android:id="@+id/button21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@android:drawable/alert_light_frame"
            android:onClick="exone"
            android:text="@string/exone" />


        <Button
            android:id="@+id/button13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_gravity="fill_horizontal"
            android:layout_row="5"
            android:background="@android:drawable/alert_light_frame"
            android:onClick="matsushima"
            android:text="@string/matsushima" />


    </GridLayout>

</RelativeLayout>
